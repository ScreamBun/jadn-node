{
  "name": "jadnschema",
  "version": "0.1.9",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "./lib/api.js",
  "bin": "./bin/jadnschema.js",
  "scripts": {
    "bin": "ts-node -r @babel/register ./bin/jadnschema.ts",
    "build": "npm run tsc",
    "build:dev": "tsc --outDir dist",
    "clean": "ts-clean -d bin, lib -e .js,.d.ts",
    "docs": "docs:md",
    "docs:html": "rimraf docs-html && typedoc --options ./config/tsdoc.json --out docs-html lib/api.ts",
    "docs:md": "rimraf docs-md && typedoc --options ./config/tsdoc.json  --out docs-md --plugin typedoc-plugin-markdown lib/api.ts",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint-errors": "eslint . --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run tsc",
    "prod": "tsc && node ./bin/jadnschema.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:basic": "cross-env NODE_ENV=test jest -i test/basic.spec",
    "test:translate": "cross-env NODE_ENV=test jest -i test/translation.spec",
    "test:validate": "cross-env NODE_ENV=test jest -i test/validate.spec",
    "tsc": "npm run clean && tsc"
  },
  "dependencies": {
    "debug": "^4.2.0",
    "fs-extra": "^9.0.1",
    "markdown-table": "^2.0.0",
    "named-js-regexp": "^1.3.5",
    "optionator": "^0.9.1",
    "pluralize": "^8.0.0",
    "table": "^6.0.3",
    "url-parse": "^1.4.7",
    "v8-compile-cache": "^2.2.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.3",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "^14.14.7",
    "@types/pluralize": "0.0.29",
    "@types/table": "^6.0.0",
    "@types/url-parse": "^1.4.3",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.13.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-clean": "^1.0.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typedoc": "0.17.0-3",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "jadn",
    "schema",
    "validation"
  ],
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/test/jest.transform.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "files": [
    "bin/**.js",
    "bin/**.d.ts",
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ]
}
