    "prepare": "yon run tsc",
    "prepublish": "yon run tsc",
    "prod": "tsc && node ./bin/jadnschema.js",
    "test": "yon run test-translate && yon run test-validate",
    "test-translate": "ts-node -r @babel/register ./tests/schema-translators.js",
    "test-validate": "ts-node -r @babel/register ./tests/schema-validator.js",
    "tsc": "rimraf lib && tsc"
  },
  "dependencies": {
