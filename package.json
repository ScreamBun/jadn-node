{
  "name": "jadnschema",
  "version": "0.1.8",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "./lib/api.js",
  "bin": "./bin/jadnschema.js",
  "scripts": {
    "bin": "ts-node -r @babel/register ./bin/jadnschema.ts",
    "build": "npm run tsc",
    "clean": "ts-clean -d bin, lib -e .js,.d.ts",
    "docs-html": "rimraf docs-html && typedoc --options ./config/tsdoc.json --out docs-html lib/api.ts",
    "docs-md": "rimraf docs-md && typedoc --options ./config/tsdoc.json  --out docs-md --plugin typedoc-plugin-markdown lib/api.ts",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint-errors": "eslint . --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run tsc",
    "prepublish": "npm run tsc",
    "prod": "tsc && node ./bin/jadnschema.js",
    "test": "npm run test-translate && npm run test-validate",
    "test-translate": "ts-node -r @babel/register ./tests/schema-translators.ts",
    "test-validate": "ts-node -r @babel/register ./tests/schema-validator.ts",
    "tsc": "npm run clean && tsc"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "markdown-table": "^2.0.0",
    "named-js-regexp": "^1.3.5",
    "optionator": "^0.9.1",
    "pluralize": "^8.0.0",
    "table": "^5.4.6",
    "v8-compile-cache": "^2.1.1",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "^14.6.0",
    "@types/pluralize": "0.0.29",
    "@types/table": "^5.0.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-promise": "^4.2.1",
    "rimraf": "^3.0.2",
    "ts-clean": "^1.0.3",
    "ts-node": "^8.10.2",
    "typedoc": "0.17.0-3",
    "typedoc-plugin-markdown": "^2.4.1",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "jadn",
    "schema",
    "validation"
  ],
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "files": [
    "bin/**.js",
    "bin/**.d.ts",
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ]
}
