{
  "name": "jadnschema",
  "version": "0.1.3",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "./lib/api",
  "bin": "./bin/jadnschema.js",
  "type": "module",
  "scripts": {
    "build": "yon run tsc",
    "clean": "ts-clean -d bin, lib -e .js,.d.ts",
    "docs-html": "rimraf docs-html && typedoc --options ./config/tsdoc.json --out docs-html lib/api.ts",
    "docs-md": "rimraf docs-md && typedoc --options ./config/tsdoc.json  --out docs-md --plugin typedoc-plugin-markdown lib/api.ts",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint-errors": "eslint . --cache --quiet --ext .js,.jsx,.ts,.tsx",
    "prepare": "yon run tsc",
    "prepublish": "yon run tsc",
    "prod": "tsc && node ./bin/jadnschema.js",
    "test": "yon run test-translate && yon run test-validate",
    "test-translate": "ts-node -r @babel/register ./tests/schema-translators.ts",
    "test-validate": "ts-node -r @babel/register ./tests/schema-validator.ts",
    "tsc": "yon run clean && tsc"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "fs-extra": "^9.0.1",
    "js-beautify": "^1.11.0",
    "less": "^3.11.3",
    "lodash": "^4.17.15",
    "markdown-table": "^2.0.0",
    "named-js-regexp": "^1.3.5",
    "optionator": "^0.9.1",
    "pluralize": "^8.0.0",
    "table": "^5.4.6",
    "ts-clean": "^1.0.3",
    "twig": "^1.15.1",
    "v8-compile-cache": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/plugin-transform-typescript": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/less": "^3.0.1",
    "@types/lodash": "^4.14.155",
    "@types/markdown-table": "^2.0.0",
    "@types/node": "^14.0.13",
    "@types/pluralize": "0.0.29",
    "@types/table": "^5.0.0",
    "@types/twig": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-compat": "^3.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-eslint-plugin": "^2.2.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.0-3",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yarn-or-npm": "^3.0.1"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib"
  },
  "files": [
    "bin/**.js",
    "bin/**.d.ts",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.twig"
  ],
  "keywords": [
    "jadn",
    "schema",
    "validation"
  ]
}
